    "craco-antd": "^1.19.0",
    "craco-fast-refresh": "^1.0.2",
    "craco-less": "^1.17.0",
    "craco-vtk": "^1.0.2",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "fs": "^0.0.1-security",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-scripts": "3.4.3",
    "simple-progress-webpack-plugin": "^1.1.2",
    "terser-webpack-plugin": "^5.0.0",
